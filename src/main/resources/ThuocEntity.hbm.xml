<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.doan.hcpharma.model.ThuocEntity" table="thuoc" schema="qlnhathuoc">
        <id name="maThuoc">
            <column name="maThuoc" sql-type="int"/>
        </id>
        <property name="donGia">
            <column name="donGia" sql-type="double" precision="-1"/>
        </property>
        <property name="donGiaNhap">
            <column name="donGiaNhap" sql-type="double" precision="-1"/>
        </property>
        <property name="donViTinh">
            <column name="donViTinh" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="doiTuongSD">
            <column name="doiTuongSD" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="moTa">
            <column name="moTa" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="ngayHh">
            <column name="ngayHH" sql-type="date" not-null="true"/>
        </property>
        <property name="keDon">
            <column name="keDon" sql-type="boolean" not-null="true"/>
        </property>
<!--        <property name="hinhAnh">-->
<!--            <column name="hinhAnh" sql-type="varchar(255)" not-null="true"/>-->
<!--        </property>-->
        <property name="ngaySx">
            <column name="ngaySX" sql-type="date" not-null="true"/>
        </property>
        <property name="tenThuoc">
            <column name="tenThuoc" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="xuatXu">
            <column name="xuatXu" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="maKhuVuc">
            <column name="maKhuVuc" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="maLoaiThuoc">
            <column name="maLoaiThuoc" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>